{
  "presets": [
    ["env"],
    "flow",
    "stage-2",
    "react",
    "react-boilerplate"
  ],
  "ignore": "node_modules",
  "plugins": [
    ["transform-runtime", {"polyfill": false, "regenerator": true}],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
          "Components": "components",
          "Assets": "assets"
      }
    }]
  ]
}
