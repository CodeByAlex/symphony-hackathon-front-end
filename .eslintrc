{
    "extends": ["plugin:react/recommended", "plugin:import/errors", "plugin:import/warnings"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "settings": {
        "import/resolver": "webpack"
    },
    "plugins": [
        "react",
        "import"
    ],
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "es6": true
    },
    "rules": {
        "space-before-function-paren": "off",
        "react/prefer-stateless-function": "warn",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "linebreak-style": "off",
        "global-require": "off",
        "generator-star-spacing": 2,
        "semi": "warn",
        "arrow-body-style": "off",
        "arrow-parens": 2,
        "block-scoped-var": 2,
        "brace-style": [2, "1tbs", {"allowSingleLine": true}],
        "camelcase": [2, {"properties": "never"}],
        "comma-dangle": [2, "always-multiline"],
        "comma-spacing": [2, {"before": false, "after": true}],
        "comma-style": [2, "last"],
        "consistent-return": 0,
        "curly": [2, "all"],
        "default-case": 0,
        "dot-notation": [2, {"allowKeywords": true}],
        "eol-last": 2,
        "eqeqeq": 2,
        "func-names": 2,
        "guard-for-in": 2,
        "indent": ["error", 4],
        "import/no-mutable-exports": 1,
        "import/prefer-default-export": 0,
        "jsx-quotes": [2, "prefer-single"],
        "key-spacing": [1, {"beforeColon": false, "afterColon": true}],
        "keyword-spacing": 2,
        "max-len": [1, { "tabWidth": 4, "code": 120 } ],
        "new-cap": [2, {"newIsCap": true, "capIsNewExceptions": ["SortableContainer", "SortableElement"]}],
        "newline-after-var": 0,
        "no-alert": 2,
        "no-caller": 2,
        "no-cond-assign": [2, "except-parens"],
        "no-console": [2, {"allow": ["error", "warn", "log"]}],
        "no-constant-condition": 2,
        "no-debugger": 0,
        "no-div-regex": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 0,
        "no-else-return": 2,
        "no-empty": [2, {"allowEmptyCatch": true}],
        "no-empty-character-class": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-implied-eval": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-loop-func": 2,
        "no-mixed-operators": 1,
        "no-multi-str": 2,
        "no-multiple-empty-lines": 2,
        "no-native-reassign": 2,
        "no-nested-ternary": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
        "no-param-reassign": 1,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-spaced-func": 2,
        "no-sparse-arrays": 2,
        "no-throw-literal": 2,
        "no-trailing-spaces": 2,
        "no-undefined": 0,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 2,
        "no-unreachable": 2,
        "no-unused-expressions": [2, { "allowShortCircuit": true, "allowTernary": true }],
        "no-unused-vars": 0,
        "no-use-before-define": 1,
        "no-var": 2,
        "no-void": 2,
        "no-with": 2,
        "object-shorthand": [2, "methods", {"avoidQuotes": true}],
        "one-var": [2, "never"],
        "operator-assignment": [2, "always"],
        "operator-linebreak": [2, "before"],
        "padded-blocks": 0,
        "prefer-arrow-callback": 1,
        "quotes": [2, "single", "avoid-escape"],
        "radix": 2,
        "react/display-name": 0,
        "react/jsx-first-prop-new-line": [2, "multiline"],
        "react/jsx-boolean-value": [2,"never"],
        "react/jsx-indent": [2,4],
        "react/jsx-indent-props": [2,4],
        "react/jsx-equals-spacing": [2, "never"],
        "react/jsx-no-undef": 2,
        "react/jsx-sort-prop-types": 0,
        "react/jsx-sort-props": 0,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/jsx-wrap-multilines": 1,
        "react/jsx-closing-bracket-location": 0,
        "react/wrap-multilines": 0,
        "react/no-array-index-key": 0,
        "react/no-did-mount-set-state": 0,
        "react/no-did-update-set-state": 2,
        "react/no-multi-comp": 0,
        "react/no-unknown-property": 2,
        "react/prop-types": 0,
        "react/self-closing-comp": 2,
        "semi-spacing": [2, {"before": false, "after": true}],
        "space-before-blocks": [2, "always"],
        "object-curly-spacing": ["error", "always"],
        "space-infix-ops": [2, {"int32Hint": false}],
        "space-unary-ops": [2, {"words": true, "nonwords": false}],
        "use-isnan": 2,
        "vars-on-top": 2,
        "wrap-iife": [2, "any"],
        "yoda": [2, "never", {"exceptRange": true}]
    }
}
